<?php
	$client = new SoapClient("http://127.0.0.1:8080/axis2/services/Version?wsdl"); // EdereÃ§o do servidor a ser consumido (lido)
	try {
	  	$function = 'getVersion';
		 
		$result = $client->__soapCall($function,"arg0");
		 
		echo 'Response: ';
		
		print_r($result);
		
	} catch (Exception $e) {
	  echo "ERRO: " . $e->getMessage();
	}
?>
