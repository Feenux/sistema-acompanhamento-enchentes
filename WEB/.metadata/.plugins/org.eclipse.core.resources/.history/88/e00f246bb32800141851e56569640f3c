<?php
	$client = new SoapClient("http://ec2-54-207-68-21.sa-east-1.compute.amazonaws.com:8080/axis2/services/helloworld?wsdl"); // EdereÃ§o do servidor a ser consumido (lido)
	try {
	  	$function = 'getVersion';
		 
		$result = $client->__soapCall($function,"arg0");
		 
		echo 'Response: ';
		
		print_r($result);
		
	} catch (Exception $e) {
	  echo "ERRO: " . $e->getMessage();
	}
?>
