<?php
	echo "teste";

	$client = new SoapClient("http://ec2-54-207-68-21.sa-east-1.compute.amazonaws.com:8080/axis2/services/helloworld?wsdl"); // EdereÃ§o do servidor a ser consumido (lido)
	try {
	  	$function = 'arg0';
		 
		$arguments= array('ConvertTemp' => array(
		                        'Temperature'   => 31,
		                        'FromUnit'      => 'degreeCelsius',
		                        'ToUnit'        => 'degreeFahrenheit'
		                ));
		$options = array('location' => 'http://www.webservicex.net/ConvertTemperature.asmx');
		 
		$result = $client->__soapCall($function);
		 
		echo 'Response: ';
		
		print_r($result);
	} catch (Exception $e) {
	  echo "ERRO: " . $e->getMessage();
	}
?>
